{
    "name": "sanatorium/analytics",
    "type": "platform-extension",
    "description": "Package to retrieve Google Analytics data for Cartalyst Platform",
    "authors": [
        {
            "name": "Sanatorium",
            "email": "info@sanatoriu.ninja"
        },
        {
            "name": "rozklad",
            "email": "jan.rozklad@gmail.com",
            "homepage": "http://rozklad.me"
        },
        {
            "name": "mattluk",
            "email": "matejlukas12@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "cartalyst",
        "platform",
        "extension",
        "madeinsane"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "cartalyst/composer-installers": "1.2.*",
        "platform/foundation": ">=2.0 <5.0",
        "spatie/laravel-analytics": "1.3.*"
    },
    "autoload": {
        "classmap": [
            "database/migrations",
            "database/seeds"
        ],
        "psr-4": {
            "Sanatorium\\Analytics\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}